{
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Dracula Theme",
  "workbench.iconTheme": "material-icon-theme",
  "editor.minimap.enabled": false,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "editor.fontSize": 15,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "terminal.integrated.fontSize": 15,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "diffEditor.renderSideBySide": false,
  "editor.lineNumbers": "relative",
  "editor.lineHeight": 40,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
  },
  "php-cs-fixer.config": "~/.php_cs",
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.allowRisky": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.lastDownload": 1646451439155,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    // Hide everything in /public, except "index.php"
    "**/public/[abcdefghjklmnopqrstuvwxyz]*": true,
    "**/public/i[abcdefghijklmopqrstuvwxyz]*": true,
    // Hide everything in /vendor, except "laravel" folder.
    "**/vendor/[abcdefghijkmnopqrstuvwxyz]*": true,
    "**/vendor/l[bcdefghijklmnopqrstuvwxyz]*": true,
    "storage/framework/views": true
  },
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "workbench.editor.showTabs": "single",
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "none",
  "editor.matchBrackets": "never",
  "workbench.sideBar.location": "right",
  "editor.quickSuggestionsDelay": 0,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "vetur.format.options.tabSize": 4,
  "vetur.validation.template": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "vue-html",
    "markdown"
  ],
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "editor.renderWhitespace": "all",
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.confirmDelete": false,
  "vim.useSystemClipboard": true,
  "workbench.statusBar.visible": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[lua]": {
    "editor.defaultFormatter": "yinfei.luahelper"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.guides.indentation": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.layoutControl.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "breadcrumbs.enabled": false,
  "window.title": " ${dirty}",
  "vscode_custom_css.imports": [
    "file:///Users/rb/.dotfiles/vscode/hide-top-bar.css"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "workbench.activityBar.location": "hidden",
  "editor.largeFileOptimizations": false
}